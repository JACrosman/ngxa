{
  "name": "ngx-api",
  "description": "Makes REST api calls easy in ngrx",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JACrosman/ngxa.git"
  },
  "keywords": [
    "ngx-api",
    "rest",
    "redux",
    "state",
    "rxjs",
    "angular",
    "ngx",
    "angular2",
    "state-mangement"
  ],
  "author": "Joshua Crosman",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JACrosman/ngxa/issues"
  },
  "homepage": "https://github.com/JACrosman/ngxa/#readme",
  "packageScope": "@ngxa",
  "packages": [
    "packages/ngrx"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --port 4201",
    "start:server": "node server/server",
    "build": "yarn build:packages",
    "build:packages": "ng-packagr -p packages/ngrx/ng-package.json",
    "build:integration": "ng build --prod --app 1",
    "prettier": "prettier",
    "format": "prettier --write *.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ngrx/entity": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngrx/store-devtools": "^5.2.0",
    "body-parser": "^1.18.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.2",
    "prettier": "^1.12.1",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tsickle": "^0.27.5",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
